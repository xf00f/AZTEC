    "build": "./node_modules/.bin/rimraf ./lib && ./node_modules/.bin/cross-env BABEL_ENV=production ./node_modules/.bin/babel ./src --copy-files --out-dir ./lib",
    "build:docs": "./node_modules/.bin/jsdoc -c ./.jsdoc.json",
    "publish:docs": "npm run build:docs && ./node_modules/.bin/push-dir --dir ./docs --branch gh-pages --remote origin --cleanup"
